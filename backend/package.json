{
  "name": "conversational-agent-backend",
  "version": "1.0.0",
  "description": "Backend server for conversational agent workshop with Deepgram integration",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "echo 'Tests not implemented yet'",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "deepgram",
    "websocket",
    "audio",
    "transcription",
    "real-time",
    "typescript"
  ],
  "author": "Vini Ganancio <contato@viniganancio.dev>",
  "license": "MIT",
  "dependencies": {
    "@deepgram/sdk": "^3.4.0",
    "express": "^4.18.2",
    "socket.io": "^4.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
